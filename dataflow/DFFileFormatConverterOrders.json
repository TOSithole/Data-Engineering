{
	"name": "DFFileFormatConverterOrders",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "OrdersCSV",
						"type": "DatasetReference"
					},
					"name": "orders",
					"description": "Add source dataset"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "OrdersParquet",
						"type": "DatasetReference"
					},
					"name": "OrdersParquet"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          order_id as integer,",
				"          order_date as timestamp,",
				"          order_customer_id as integer,",
				"          order_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> orders",
				"orders sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> OrdersParquet"
			]
		}
	}
}